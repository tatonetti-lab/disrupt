        "outcome_measures": [
            {
                "timeframe": "Up to 1 year",
                "name": "Comparison of full abbreviated magnetic resonance imaging (AMRI), simulated unenhanced AMRI, and ultrasound (US) for hepatocellular carcinoma (HCC) detection",
                "description": "Will be assessed per-patient sensitivity, specificity, positive predictive (PPV), negative predictive value (NPV) and accuracy of full AMRI versus (vs.) simulated unenhanced AMRI vs. US for HCC screening.",
                "type_code": "PRIMARY"
            },
            {
                "timeframe": "Up to 12 months from completion of imaging",
                "name": "Reader reliability of screening modalities",
                "description": "Will assess the inter- and intra-reader reliability of full AMRI vs. simulated unenhanced AMRI vs. US for HCC screening.",
                "type_code": "PRIMARY"
            }
        ],

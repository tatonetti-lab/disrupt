        "eligibility": {
            "unstructured": [
                {
                    "inclusion_indicator": true,
                    "display_order": 1,
                    "description": "Patients must have an imaging abnormality that necessitated a core needle biopsy"
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 2,
                    "description": "The imaging abnormality must have been categorized as Breast Imaging-Reporting and Data System (BIRADS) level 1-4 lesion"
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 3,
                    "description": "There is documented concordance* between the initial breast imaging finding and the core biopsy pathology report; the core needle biopsy must contain FEA or IPWA, according to the local pathologist; (it is possible that the central pathology review which is done after the patient is registered on this protocol will have a diagnosis discrepant from that made by the original institution\u2019s pathologist; in that case, the study team will communicate this to the original institution\u2019s site investigator within one week of the date of the central pathology review having been finalized); patients may have a personal history of prior or concurrent fibroadenoma and a prior history of proliferative breast lesions with or without atypia\r\n* Concordance is a determination by the radiologist (or his or her covering provider) performing an image-guided core needle biopsy that the pathology report from this procedure corresponds to the imaging appearance of a given lesion and that the said lesion\u2019s most representative portion has been sampled"
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 4,
                    "description": "Patients must be registered on study within 100 days of the core needle biopsy"
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 5,
                    "description": "Patients must have an ability to understand and the willingness to sign a written informed consent document"
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 6,
                    "description": "Personal history and/or concomitant diagnosis of invasive breast cancer or DCIS"
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 7,
                    "description": "Palpable abnormality diagnosed by core needle biopsy to be FEA or IPWA"
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 8,
                    "description": "Pathologic nipple discharge associated with IPWA (spontaneous bloody or clear persistent single duct discharge)"
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 9,
                    "description": "A BIRADS 5 lesion"
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 10,
                    "description": "Discordance between the initial breast imaging finding and the core biopsy pathology report"
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 11,
                    "description": "The presence of atypical ductal hyperplasia (ADH) on core biopsy"
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 12,
                    "description": "Known current pregnancy; a pregnancy test is not required for this exclusion criteria"
                },
                {
                    "inclusion_indicator": false,

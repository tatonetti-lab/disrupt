        "eligibility": {
            "unstructured": [
                {
                    "inclusion_indicator": true,
                    "display_order": 0,
                    "description": "Biochemical recurrence (BCR) within 3 years of radical prostatectomy (RP) or definitive RT and no distant metastasis by standard CT imaging and bone scintigraphy, or locoregional recurrence (including lymph nodes) assessed by CT or multi-parametric magnetic resonance imaging (MRI) and confirmed with negative biopsy in case of prior RT."
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 1,
                    "description": "In case of BCR after RP all the following criteria should apply: a. PSA \u22650.2 ng/mL, b. PSA Doubling Time (PSADT) >3 months and <12 months"
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 2,
                    "description": "In case of BCR after RT all the following criteria should apply: a. PSA >nadir + 2 ng/mL, b. PSADT >3 months and <12 months"
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 3,
                    "description": "ECOG performance status \u22642."
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 4,
                    "description": "Laboratory values obtained \u226430 days prior to first vaccination: Hemoglobin \u22655.6 mmol/L; Absolute granulocyte count \u22651.5 x 109 /L, Platelets \u2265100 x 109 /L., Total bilirubin \u22641.5 x upper limit of normal (ULN)."
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 5,
                    "description": "Creatinine \u22641.5 x ULN."
                },
                {
                    "inclusion_indicator": true,
                    "display_order": 6,
                    "description": "Alanine aminotransferase (ALT), aspartate aminotransferase (AST) and alkaline phosphatase (ALP) \u22642.5 x ULN."
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 7,
                    "description": "Patients who are receiving androgen-deprivation therapy or considered a candidate for immediate anti-androgen deprivation therapy (ADT) as judged by the investigator."
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 8,
                    "description": "Patients who have received prior ADT are not eligible with the exception of those that received ADT \u226436 months in duration and \u22659 months before randomization and administered only in the neoadjuvant/adjuvant setting."
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 9,
                    "description": "Patient is planned for salvage therapy with RT or radical prostatectomy."
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 10,
                    "description": "Castrate level of serum testosterone <50 ng/dL at screening."
                },
                {
                    "inclusion_indicator": false,
                    "display_order": 11,
                    "description": "PSA >10 ng/mL"
                }
            ],

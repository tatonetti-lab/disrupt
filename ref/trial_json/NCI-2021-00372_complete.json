        "outcome_measures": [
            {
                "timeframe": "Up to 5 years",
                "name": "Cost-effectiveness of HBP-AMRI vs. US for HCC screening",
                "description": "Using a micro simulation model, will estimate and compare the expected costs and quality-adjusted survival of four strategies for HCC screening: HBP-AMRI alone, US alone, HBP-AMRI with AFP, US with AFP).",
                "type_code": "PRIMARY"
            },
            {
                "timeframe": "Up to 5 years",
                "name": "Negative predictive value of HCC detection by HBP-AMRI vs. US",
                "description": "The point estimates of sensitivity, specificity, PPV and NPV along with the corresponding 95% confidence intervals will be calculated for HBP-AMRI and US. The difference in sensitivity between HBP-AMRI and US accounting for correlation of the two diagnostic approaches in the same patient will be tested using a two-sided McNemar test.",
                "type_code": "PRIMARY"
            },
            {
                "timeframe": "Up to 5 years",
                "name": "Positive predictive value of HCC detection by HBP-AMRI vs. US",
                "description": "The point estimates of sensitivity, specificity, PPV and NPV along with the corresponding 95% confidence intervals will be calculated for HBP-AMRI and US. The difference in sensitivity between HBP-AMRI and US accounting for correlation of the two diagnostic approaches in the same patient will be tested using a two-sided McNemar test.",
                "type_code": "PRIMARY"
            },
            {
                "timeframe": "Up to 5 years",
                "name": "Sensitivity of circulating tumor deoxyribonucleic acid (ctDNA) and serum alpha-fetoprotein (AFP) alone and combined with HBP-AMRI or US for HCC screening",
                "description": null,
                "type_code": "PRIMARY"
            },
            {
                "timeframe": "Up to 5 years",
                "name": "Sensitivity of hepatocellular carcinoma (HCC) detection by hepatobiliary phase abbreviated magnetic resonance imaging (HBP-AMRI) versus (vs.) ultrasound (US)",
                "description": "The point estimates of sensitivity, specificity, positive predictive value (PPV) and negative predictive value (NPV) along with the corresponding 95% confidence intervals will be calculated for HBP-AMRI and US. The difference in sensitivity between HBP-AMRI and US accounting for correlation of the two diagnostic approaches in the same patient will be tested using a two-sided McNemar test.",
                "type_code": "PRIMARY"
            },
            {
